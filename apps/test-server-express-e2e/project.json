{
    "name": "test-server-express-e2e",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "implicitDependencies": ["test-server-express"],
    "targets": {
        "e2e": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
            "options": {
                "jestConfig": "apps/test-server-express-e2e/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "apps/test-server-express-e2e/**/*.{js,ts}"
                ]
            }
        }
    }
}
