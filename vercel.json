{
    "version": 2,
    "builds": [
        {
            "src": "dist/apps/server/main.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/apps/server/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/apps/server/main.js"
        }
    ]
}
